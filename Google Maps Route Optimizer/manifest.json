
{
  "manifest_version": 3,
  "name": "Google Maps Route Optimizer",
  "description": "Assist in optimizing routes with multiple destinations",
  "version": "1.0.0",
  "author": "Alec Maly",
  "content_scripts": [
    {
      "matches": ["*://*.google.com/maps/*"],
      "js": ["inject.js"],
      "run_at": "document_end" 
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["payload.js"],
      "matches": ["*://*.google.com/*"]
    }
  ],
  "homepage_url": "https://maps.google.com/",
  "permissions": [
    "activeTab"
  ]
}
